{
  "name": "tabbed",
  "version": "0.0.1",
  "private": true,
  "description": "Save your browser tabs",
  "scripts": {
    "build": "rollup -c",
    "clean": "ts-node -P bin/tsconfig.json bin/clean",
    "format": "run-p format:*",
    "format:packagejson": "sort-package-json",
    "format:prettier": "prettier --write \"**/*.{ts,tsx,js}\"",
    "start": "rollup -c -w",
    "test": "run-s test:*",
    "test:jest": "jest --coverage",
    "test:lint": "eslint . --ext .js,.jsx,.ts,.tsx,.svelte",
    "test:svelte": "svelte-check",
    "test:ts": "tsc --project tsconfig.json --noEmit",
    "watch": "run-p watch:*",
    "watch:jest": "jest --watch"
  },
  "dependencies": {
    "autoprefixer": "^10.2.1",
    "postcss": "^8.2.4",
    "svelte": "^3.31.2",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.0",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-svelte3": "^3.2.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.36.2",
    "rollup-plugin-chrome-extension": "^3.5.2",
    "rollup-plugin-empty-dir": "^1.0.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-zip": "^1.0.1",
    "sort-package-json": "1.48",
    "svelte-check": "^1.0.0",
    "svelte-jester": "^1.3.0",
    "svelte-preprocess": "^4.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^10.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=15"
  }
}
