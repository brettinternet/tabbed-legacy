{
  "name": "tabbed",
  "version": "0.0.1",
  "private": true,
  "description": "Save your browser tabs",
  "scripts": {
    "build": "rollup -c",
    "clean": "ts-node -P bin/tsconfig.json bin/clean",
    "format": "run-p format:*",
    "format:packagejson": "sort-package-json",
    "format:prettier": "prettier --write \"**/*.{ts,js,svelte}\"",
    "start": "rollup -c -w",
    "test": "run-s test:*",
    "test:jest": "jest --coverage",
    "test:lint": "eslint . --ext .js,.jsx,.ts,.tsx,.svelte",
    "test:svelte": "svelte-check",
    "test:ts": "tsc -p tsconfig.json --noEmit",
    "watch": "run-p watch:*",
    "watch:jest": "jest --watch"
  },
  "dependencies": {
    "autoprefixer": "^10.3.1",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.22.1",
    "focus-trap": "^6.6.0",
    "hotkeys-js": "^3.8.7",
    "lodash": "^4.17.21",
    "loglevel": "^1.7.1",
    "postcss": "^8.3.5",
    "svelte": "^3.38.3",
    "svelte-i18n": "^3.3.9",
    "tailwindcss": "^2.2.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-alias": "^3.1.4",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^2.0.1",
    "@types/body-scroll-lock": "^2.6.2",
    "@types/firefox-webext-browser": "^82.0.1",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-jest": "^27.0.6",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-svelte3": "^3.2.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "rollup": "^2.53.2",
    "rollup-plugin-chrome-extension": "^3.5.7",
    "rollup-plugin-empty-dir": "^1.0.5",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-zip": "^1.0.2",
    "sort-package-json": "^1.50.0",
    "svelte-check": "^2.2.3",
    "svelte-jester": "^1.7.0",
    "svelte-preprocess": "^4.7.4",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=15"
  }
}
