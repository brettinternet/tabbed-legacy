{
  "name": "tabbed",
  "version": "0.0.1",
  "private": true,
  "description": "Save your browser tabs",
  "scripts": {
    "build": "rollup -c",
    "clean": "ts-node -P bin/tsconfig.json bin/clean",
    "format": "run-p format:*",
    "format:packagejson": "sort-package-json",
    "format:prettier": "prettier --write \"**/*.{ts,tsx,js}\"",
    "start": "rollup -c -w",
    "test": "run-s test:*",
    "test:jest": "jest --coverage",
    "test:lint": "eslint . --ext .js,.jsx,.ts,.tsx,.svelte",
    "test:svelte": "svelte-check",
    "test:ts": "tsc --project tsconfig.json --noEmit",
    "watch": "run-p watch:*",
    "watch:jest": "jest --watch"
  },
  "dependencies": {
    "autoprefixer": "^10.2.1",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.22.1",
    "focus-trap": "^6.6.0",
    "hotkeys-js": "^3.8.7",
    "lodash": "^4.17.21",
    "postcss": "^8.2.4",
    "svelte": "^3.31.2",
    "svelte-i18n": "^3.3.9",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^6.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.0",
    "@types/body-scroll-lock": "^2.6.2",
    "@types/chrome": "^0.0.146",
    "@types/firefox-webext-browser": "^82.0.1",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-svelte3": "^3.2.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.36.2",
    "rollup-plugin-chrome-extension": "^3.5.7",
    "rollup-plugin-empty-dir": "^1.0.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-zip": "^1.0.1",
    "sort-package-json": "1.48",
    "svelte-check": "^1.0.0",
    "svelte-jester": "^1.7.0",
    "svelte-preprocess": "^4.0.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=15"
  }
}
